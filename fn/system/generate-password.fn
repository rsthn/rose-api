
(shield:method-required "GET")

(shield:validate-data input (gateway.request) (body
    token (rules
        required true
        min-length 8
        max-length 256
        pattern identifier
        check:@invalid_token (eq? (config.system.token) ($))
    )
))

(set value (crypto:unique 16))

{
    password (value)
    hashed (sentinel:password (value))
}
