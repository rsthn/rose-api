
(sentinel::privilege-required "admin")

(call "@filters")

(set value (db::scalar `
    SELECT
        COUNT(*)

    FROM ##users x
    INNER JOIN ##user_privileges up ON up.user_id=x.user_id
    INNER JOIN ##privileges p ON p.privilege_id=up.privilege_id

    WHERE {filters}
`))

(&
    count (value)
)
